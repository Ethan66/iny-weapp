{
  "name": "iny-weapp",
  "version": "0.0.1",
  "description": "iny 基础组件",
  "miniprogram": "dist",
  "scripts": {
    "predev": "rimraf dist && rimraf example/dist",
    "dev": "node build/dev.js",
    "prebuild": "rimraf dist && rimraf example/dist",
    "build": "gulp --gulpfile build/build-prod.js",
    "start": "npm run dev",
    "docs": "npx cross-env NODE_ENV=production webpack --config build/webpack.doc.prd.js",
    "lint": "eslint ./packages --ext .js",
    "release": "sh ./build/release.sh",
    "publish": "npm run build && npm publish --access public --registry=https://registry.npmjs.org"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:Ethan66/iny-weapp.git"
  },
  "keywords": [
    "wx",
    "base",
    "iny",
    "component"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js, scss}": [
      "eslint --ext",
      "git add"
    ]
  },
  "author": "landluck",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.5.5",
    "@vant/doc": "^2.5.5",
    "@vant/markdown-loader": "^2.2.0",
    "autoprefixer": "^9.6.1",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.6",
    "cross-env": "^6.0.3",
    "css-loader": "^3.2.0",
    "cssnano": "^4.1.10",
    "eslint": "^6.3.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "gulp": "^4.0.2",
    "gulp-eslint": "^6.0.0",
    "gulp-insert": "^0.5.0",
    "gulp-postcss": "^8.0.0",
    "gulp-rename": "^1.4.0",
    "gulp-sass": "^4.0.2",
    "gulp-uglify": "^3.0.2",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^2.7.0",
    "jszip": "^3.2.2",
    "less-loader": "^5.0.0",
    "lint-staged": "^8.2.1",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.16.4",
    "progress-bar-webpack-plugin": "^1.11.0",
    "rimraf": "^3.0.0",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0",
    "vue": "2.6.10",
    "vue-loader": "^15.7.1",
    "vue-router": "^3.1.1",
    "vue-template-compiler": "2.6.10",
    "webpack": "^4.39.1",
    "webpack-cli": "^3.3.6",
    "webpack-serve": "^2.0.3"
  }
}
