.iny-cell{position:relative;display:flex;align-items:center;width:100%;padding:10px 13px;font-size:14px;line-height:24px;color:#333;background-color:#fff;box-sizing:border-box}.iny-cell--border:after{position:absolute;box-sizing:border-box;transform-origin:center;content:" ";pointer-events:none;top:auto;right:0;bottom:0;left:13px;border-bottom:1px solid #eee;transform:scaleY(.5)}.iny-cell-group{background-color:#fff}.iny-cell__label,.iny-cell__value{overflow:hidden;color:#a2a4a6;text-align:right;vertical-align:middle}.iny-cell__title,.iny-cell__value{flex:1}.iny-cell__label{margin-left:15px}.iny-cell__title{display:flex;align-items:center}.iny-cell__left-icon-wrap,.iny-cell__right-icon-wrap{display:flex;height:24px;font-size:16px;align-items:center}.iny-cell__left-icon-wrap{margin-right:5px}.iny-cell__right-icon-wrap{margin-left:5px;color:#333;color:#a2a4a6}.iny-cell__left-icon{line-height:24px;vertical-align:middle}.iny-cell__right-icon{line-height:24px}.iny-cell--clickable.iny-cell--hover{background-color:#f2f3f5}