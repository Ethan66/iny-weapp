.wrap {
  margin: 0;
  display: flex;
  flex-wrap: wrap;
  padding: 15px;
  text-align: center;
  border-radius: 6px;
  background: #fff;
}
.name {
  width: 70px;
  font-size: 14px;
  color: #999;
}
.inline {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  width: 80px;
  height: 80px;
  color: #666;
}